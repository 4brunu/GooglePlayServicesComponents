<metadata>
	
	<!-- Rename Namespaces -->
	<attr path="/api/package[@name='com.google.firebase']" name="managedName">Firebase</attr>
	<attr path="/api/package[@name='com.google.firebase.iid']" name="managedName">Firebase.Iid</attr>

	<!-- Remove *zz* obfuscated classes, interfaces, methods, etc. -->
	<remove-node path="/api/package/class[contains(@name, 'zz') and @name != 'zzb']" />
	<remove-node path="/api/package/interface[contains(@name, 'zz')]" />
	<remove-node path="/api/*/*/method[contains(@name, 'zz')]" />
	<remove-node path="/api/*/*/field[contains(@name, 'zz')]" />

	<!-- Fix params for some inherited parcelable types -->
	<attr path="/api/*/*/method[@name='createFromParcel']/parameter[@name='p0']" name="managedName">source</attr>
	<attr path="/api/*/*/method[@name='newArray']/parameter[@name='p0']" name="managedName">size</attr>
	<attr path="/api/*/*/method[@name='writeToParcel']/parameter[@name='p0']" name="managedName">dest</attr>
	<attr path="/api/*/*/method[@name='writeToParcel']/parameter[@name='p1']" name="managedName">flags</attr>

	<attr path="/api/package[@name='com.google.firebase.iid']/class[@name='zzb']" name="obfuscated">false</attr>

	<add-node path="/api/package[@name='com.google.firebase.iid']">
		<class abstract="false" deprecated="not deprecated" jni-extends="Landroid/app/Service;" extends="android.app.Service" extends-generic-aware="android.App.Service" final="false" name="FirebaseInstanceIdService" jni-signature="Lcom/google/firebase/iid/FirebaseInstanceIdService;" static="false" visibility="public">
			<constructor deprecated="not deprecated" final="false" name="FirebaseInstanceIdService" static="false" visibility="public" bridge="false" synthetic="false" jni-signature="()V" />
			<method abstract="false" deprecated="not deprecated" final="false" name="handleIntent" native="false" return="void" jni-return="V" static="false" synchronized="false" visibility="public"
				bridge="false" synthetic="false" jni-signature="(Landroid/content/Intent;)V">
				<parameter name="bundle" type="android.content.Intent" jni-type="Landroid/content/Intent;" />
			</method>
			<method abstract="false" deprecated="not deprecated" final="false" name="onBind" native="false" return="android.os.IBinder" jni-return="Landroid/os/IBinder;" static="false" synchronized="false" visibility="public"
				bridge="false" synthetic="false" jni-signature="(Landroid/content/Intent;)Landroid/os/IBinder;">
				<parameter name="intent" type="android.content.Intent" jni-type="Landroid/content/Intent;" />
			</method>
			<method abstract="false" deprecated="not deprecated" final="false" name="onTokenRefresh" native="false" return="void" jni-return="V" static="false" synchronized="false" visibility="public"
				bridge="false" synthetic="false" jni-signature="()V" />
		</class>
	</add-node>
</metadata>
