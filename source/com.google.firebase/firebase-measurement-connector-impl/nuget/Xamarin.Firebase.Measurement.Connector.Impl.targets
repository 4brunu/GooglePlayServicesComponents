<?xml version="1.0" encoding="UTF-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <PropertyGroup>
        <!-- Unique key for cached download -->
        <_XbdKey_firebasemeasurementconnectorimpl>firebase-16.0.1/firebasemeasurementconnectorimpl</_XbdKey_firebasemeasurementconnectorimpl>
        
        <!-- Path to .aar file within the extracted contents of the cached download -->
        <_XbdAarFile_firebasemeasurementconnectorimpl>m2repository\com\google\firebase\firebase-measurement-connector-impl\16.0.1\firebase-measurement-connector-impl-16.0.1.aar</_XbdAarFile_firebasemeasurementconnectorimpl>
        
        <!-- Full path to .aar file, assuming we had to download to the cache - we reset this later if it is in the sdk -->
        <_XbdAarFileFullPath_firebasemeasurementconnectorimpl>$(XamarinBuildDownloadDir)$(_XbdKey_firebasemeasurementconnectorimpl)\firebase-measurement-connector-impl-16.0.1.aar</_XbdAarFileFullPath_firebasemeasurementconnectorimpl>

        <!-- Assembly name to embed .aar in -->
        <_XbdAssemblyName_firebasemeasurementconnectorimpl>Xamarin.Firebase.Measurement.Connector.Impl</_XbdAssemblyName_firebasemeasurementconnectorimpl>
    </PropertyGroup>

    <ItemGroup>
        <XamarinBuildRestoreResources Include="_XbdRestoreItems_firebasemeasurementconnectorimpl"/>
    </ItemGroup>

    <Target Name="_XbdRestoreItems_firebasemeasurementconnectorimpl">
        <!-- Check if the aar file is in the android sdk already and change the path to use to it, if it's found -->
        <CreateProperty Value="$(AndroidSdkDirectory)\extras\google\$(_XbdAarFile_firebasemeasurementconnectorimpl)" Condition="Exists('$(AndroidSdkDirectory)\extras\google\$(_XbdAarFile_firebasemeasurementconnectorimpl)')">
            <Output PropertyName="_XbdAarFileFullPath_firebasemeasurementconnectorimpl" TaskParameter="Value" />
        </CreateProperty>

        <ItemGroup>
            <XamarinBuildDownload Include="$(_XbdKey_firebasemeasurementconnectorimpl)">
                <Url>https://dl.google.com/dl/android/maven2/com/google/firebase/firebase-measurement-connector-impl/16.0.1/firebase-measurement-connector-impl-16.0.1.aar</Url>
                <ToFile>firebase-measurement-connector-impl-16.0.1.aar</ToFile>
                <Kind>Uncompressed</Kind>
                <Md5>655d37ffc054d7de5ae3688cc9ff41ce</Md5>
            </XamarinBuildDownload>
        </ItemGroup>

        <ItemGroup>
            <XamarinBuildDownloadRestoreAssemblyAar Include="$(_XbdAarFileFullPath_firebasemeasurementconnectorimpl)">
                <LogicalName>__AndroidLibraryProjects__.zip</LogicalName>
                <AssemblyName>$(_XbdAssemblyName_firebasemeasurementconnectorimpl)</AssemblyName>
            </XamarinBuildDownloadRestoreAssemblyAar>
        </ItemGroup>
    </Target>
</Project>